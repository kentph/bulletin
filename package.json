    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.2.3",
    "feedparser-promised": "^2.0.1",
    "form-data": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "languagedetect": "^2.0.0",
    "moment": "^2.27.0",
    "newsapi": "^2.4.0",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.6",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.1",
