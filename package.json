  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm run test:e2e:sqlite",
    "test:e2e:sqlite": "concurrently -k --success first \"npm run start:e2etest --prefix client\" \"NODE_ENV=test REACT_APP_DB_CLIENT=sqlite npm run dev\"  \"wait-on http://localhost:4000 && wait-on http://localhost:4001 && REACT_APP_DB_CLIENT=sqlite npx wdio wdio.conf.js\"",
    "start": "node dist/index.js",
    "build": "npm install --prefix client && npm run build --prefix client && tsc",
    "dev": "NODE_ENV=development nodenv index.ts --exec ts-node-dev | pino-colada",
    "prod": "NODE_ENV=production nodenv index.ts --exec ts-node | pino-colada",
    "tsc": "tsc",
    "knex": "knex",
    "local": "npm install && npm run build && NODE_ENV=production npm start"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
