    "@types/pg": "^7.14.4",
    "@types/sqlite3": "^3.1.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@wdio/appium-service": "^6.6.0",
    "@wdio/cli": "^6.6.0",
    "@wdio/local-runner": "^6.6.0",
    "@wdio/mocha-framework": "^6.6.0",
    "@wdio/spec-reporter": "^6.6.0",
